<search>
	<engine name="The Pirate Bay">
		<param name="query">http://thepiratebay.org/search.php?q=%1</param>
		<param name="beginning">&lt;td class="vertTh"&gt;</param>
		<param name="splitter">&lt;/tr&gt;</param>
		<param name="ending">&lt;/table&gt;</param>
		
		<regexp name="name" field="1">class="detLink" title="([^"]+)"&gt;([^"]+)&lt;/a&gt;</regexp>
		<regexp name="link" match="1">&lt;td&gt;&lt;a href="([^"]+)"</regexp>
		<regexp name="info">&lt;a href="([^"]+)" class="detLink"</regexp>
		<regexp name="size">&lt;td align="right"&gt;([^"]+)&lt;/td&gt;</regexp>
		<regexp name="seeders" match="1">&lt;td align="right"&gt;([^"]+)&lt;/td&gt;</regexp>
		<regexp name="leechers" match="2">&lt;td align="right"&gt;([^"]+)&lt;/td&gt;</regexp>
	</engine>
	<engine name="Mininova">
		<param name="query">http://www.mininova.org/search/?search=%1</param>
		<param name="beginning">Leechers&lt;/a&gt;&lt;/th&gt;&lt;/tr&gt;</param>
		<param name="splitter">&lt;/tr&gt;</param>
		<param name="ending">&lt;/table&gt;</param>
		
		<regexp name="name" field="1">&lt;a href="/tor/([^"]+)"&gt;([^"]+)&lt;/a&gt;</regexp>
		<regexp name="info">&lt;a href="(/tor/\d+)"&gt;([^&lt;]+)&lt;/a&gt;</regexp>
		<regexp name="link">&lt;a href="([^"]+)" class="dl"</regexp>
		<regexp name="size">&lt;td align="right"&gt;([^"]+)&lt;/td&gt;</regexp>
		<regexp name="seeders" field="1">&lt;span class="(.)"&gt;([^"]+)&lt;/span&gt;</regexp>
		<regexp name="leechers" field="1" match="1">&lt;span class="(.)"&gt;([^"]+)&lt;/span&gt;</regexp>
	</engine>
	<engine name="BtJunkie">
		<param name="query">http://btjunkie.org/search?q=%1</param>
		<param name="beginning">class="tab_results"&gt;</param>
		<param name="splitter">&lt;/tr&gt;&lt;tr bgcolor="#FFFFFF"&gt;</param>
		<param name="ending">&lt;tr class="row_footer"&gt;</param>
		
		<regexp name="name">class="BlckUnd"&gt;([^;]+)</regexp>
		<regexp name="link">&lt;a href="([^"]+)" rel="nofollow"&gt;&lt;img src="/images/down.gif"</regexp>
		<regexp name="info">&lt;a href="([^"]+)" class="BlckUnd"&gt;</regexp>
		<regexp name="size">&lt;th width="10%" align="center"&gt;&lt;font color="#808080" style="font-weight: bold;"&gt;([^&lt;]+)&lt;/font&gt;</regexp>
		<regexp name="seeders" match="2">style="font-weight: bold;"&gt;([^&lt;]+)&lt;/font&gt;</regexp>
		<regexp name="leechers" match="3">style="font-weight: bold;"&gt;([^&lt;]+)&lt;/font&gt;</regexp>
	</engine>
	<engine name="LinuxTracker">
		<param name="query">http://linuxtracker.org/index.php?page=torrents&amp;search=%1&amp;category=0&amp;active=1</param>
		<param name="beginning">Cat.</param>
		<param name="splitter">&lt;/tr&gt;</param>
		<param name="ending">Back To Top</param>
		
		<regexp name="name">title="View details: ([^"]+)"&gt;</regexp>
		<regexp name="link">&lt;a href="(download.php\?id=[^"]+)"</regexp>
		<regexp name="info">&lt;a href="(index.php\?page=torrent-details[^"]+)"</regexp>
		<regexp name="size"></regexp>
		<regexp name="seeders">Click here to view peers details"&gt;(\d+)&lt;/a&gt;</regexp>
		<regexp name="leechers" match="1">Click here to view peers details"&gt;(\d+)&lt;/a&gt;</regexp>
	</engine>
