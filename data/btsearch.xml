	<engine name="BtJunkie">
		<param name="query">http://btjunkie.org/search?q=%1</param>
		<param name="beginning">class="tab_results"&gt;</param>
		<param name="splitter">&lt;/tr&gt;&lt;tr bgcolor="#FFFFFF"&gt;</param>
		<param name="ending">&lt;tr class="row_footer"&gt;</param>
		
		<regexp name="name">class="BlckUnd"&gt;([^;]+)</regexp>
		<regexp name="link">&lt;a href="([^"]+)" rel="nofollow"&gt;&lt;img src="/images/down.gif"</regexp>
		<regexp name="info">&lt;a href="([^"]+)" class="BlckUnd"&gt;</regexp>
		<regexp name="size">&lt;th width="10%" align="center"&gt;&lt;font color="#808080" style="font-weight: bold;"&gt;([^&lt;]+)&lt;/font&gt;</regexp>
		<regexp name="seeders" match="2">style="font-weight: bold;"&gt;([^&lt;]+)&lt;/font&gt;</regexp>
		<regexp name="leechers" match="3">style="font-weight: bold;"&gt;([^&lt;]+)&lt;/font&gt;</regexp>
	</engine>
	<engine name="LinuxTracker">
		<param name="query">http://linuxtracker.org/index.php?page=torrents&amp;search=%1&amp;category=0&amp;active=1</param>
		<param name="beginning">Cat.</param>
		<param name="splitter">&lt;/tr&gt;</param>
		<param name="ending">&lt;/table&gt;</param>
		
		<regexp name="name">title="View details: ([^"]+)"&gt;</regexp>
		<regexp name="link">&lt;a href="(/download.php\?id=[^"]+)"</regexp>
		<regexp name="info">&lt;a href="(index.php\?page=torrent-details[^"]+)"</regexp>
		<regexp name="size"></regexp>
		<regexp name="seeders">&lt;td align="center" width="30" class="[^"]+" style="text-align: center;">(\d+)&lt;/td&gt;</regexp>
		<regexp name="leechers" match="1">&lt;td align="center" width="30" class="[^"]+" style="text-align: center;">(\d+)&lt;/td&gt;</regexp>
	</engine>
	<engine name="CZTorrent">
		<param name="query">http://tracker.cztorrent.net/torrents/?s=%1&amp;t=0</param>
		<param name="beginning">&lt;table id='torrenty'</param>
		<param name="splitter">&lt;/tr&gt;</param>
		<param name="ending">&lt;/table&gt;</param>
		
		<regexp name="name">&lt;td class='detaily'&gt;&lt;a href='http://tracker\.cztorrent\.net/[^']+'&gt;([^&lt;]+)&lt;/a&gt;</regexp>
		<regexp name="link">href='(http://tracker\.cztorrent\.net/download/[^']+)'</regexp>
		<regexp name="size">Velikost: ([^\|]+)</regexp>
		<regexp name="seeders">&lt;span class='peers_\d'&gt;(\d+)&lt;</regexp>
		<regexp name="leechers" match="1">&lt;span class='peers_\d'&gt;(\d+)&lt;</regexp>
	</engine>
	<engine name="TorrentReactor">
		<param name="query">http://www.torrentreactor.net/search.php?words=%1</param>
		<param name="beginning">Torrentreactor search results:</param>
		<param name="splitter">&lt;tr class="torrent_</param>
		<param name="ending">Search results on other respected sites</param>
		
		<regexp name="name">&lt;a href="/torrents/[^"]+"&gt;([^\n]+)</regexp>
		<regexp name="link">&lt;a rel="nofollow" href="([^"]+)"</regexp>
		<regexp name="info">&lt;a href="(/torrents/[^"]+)"</regexp>
		<regexp name="size" match="1">&lt;td&gt;([^&lt;]+)</regexp>
		<regexp name="seeders" match="2">&lt;td&gt;([^&lt;]+)</regexp>
		<regexp name="leechers" match="3">&lt;td&gt;([^&lt;]+)</regexp>
	</engine>
	<engine name="BitTorrentMonster">
		<param name="query">http://www.btmon.com/torrent/%1</param>
		<param name="beginning">&lt;table class="torr_table"&gt;</param>
		<param name="splitter">&lt;tr&gt;</param>
		<param name="ending">&lt;/table&gt;</param>
		
		<regexp name="name">.torrent.html"&gt;([^&lt;]+)&lt;/a&gt;</regexp>
		<regexp name="link">&lt;a class="down_torr" href="([^"]+\.torrent)"</regexp>
		<regexp name="info"> href="([^"]+\.torrent\.html)"&gt;</regexp>
		<regexp name="size" match="1">&lt;td class="nw"&gt;([^&lt;]+)&lt;/td&gt;</regexp>
		<regexp name="seeders">&lt;td class="seeders"&gt;([0-9]+)&lt;/td&gt;</regexp>
		<regexp name="leechers" match="1">&lt;td&gt;([0-9]+)&lt;/td&gt;</regexp>
	</engine>
	<engine name="EZTV">
		<param name="query">http://eztv.it/search/</param>
		<param name="beginning">Television Show Releases</param>
		<param name="splitter">&lt;/tr&gt;</param>
		<param name="ending">&lt;center&gt;</param>
		<param name="postdata">SearchString1=%1&amp;SearchString=&amp;search=Search</param>
		
		<regexp name="name">class="epinfo"&gt;([^&lt;]+)&lt;</regexp>
		<regexp name="link">&lt;a href="(http://torrent\.zoink\.it/[^"]+)"</regexp>
		<regexp name="info">&lt;a href="(/shows/[^"]+)"</regexp>
	</engine>
